<programme>
	<classe>
		<type nom="INTEGER"/>
		<attribut est_defaut="oui">
			<nom_attribut nom="$val_int"/>
			<type nom="INTEGER"/>
			<type nom="$integer"/>
		</attribut>
	</classe>
	<classe>
		<type nom="BOOLEAN"/>
		<attribut est_defaut="oui">
			<nom_attribut nom="$val_bool"/>
			<type nom="BOOLEAN"/>
			<type nom="$boolean"/>
		</attribut>
		<attribut>
			<nom_attribut nom="$not"/>
			<type nom="BOOLEAN"/>
			<type nom="BOOLEAN"/>
			<routine>
				<signature/>
				<declare_var nom="Result">
					<type nom="BOOLEAN"/>
				</declare_var>
				<instruction>
					<instr_affect>
						<expression_scalaire type="var">
							<var nom="$val_bool" src="Result"/>
						</expression_scalaire>
						<expression>
							<expression_un type="not">
								<expression_scalaire type="var">
									<var nom="$val_bool"/>
								</expression_scalaire>
							</expression_un>
						</expression>
					</instr_affect>
				</instruction>
			</routine>
		</attribut>
