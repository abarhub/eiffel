<a name="329" href="#329">329</a> 		<b>private</b> <a href="../../../../../org/apache/bcel/generic/InstructionHandle.html">InstructionHandle</a>[] _getSuccessors(){
<a name="330" href="#330">330</a> 			<b>final</b> <a href="../../../../../org/apache/bcel/generic/InstructionHandle.html">InstructionHandle</a>[] empty = <b>new</b> <a href="../../../../../org/apache/bcel/generic/InstructionHandle.html">InstructionHandle</a>[0];
