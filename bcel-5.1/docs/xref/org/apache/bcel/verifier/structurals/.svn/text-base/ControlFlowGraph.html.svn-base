<a name="341" href="#341">341</a> <font color="#329900"><i>//TODO: remove</font></i>
<a name="342" href="#342">342</a> <b>throw</b> <b>new</b> <a href="../../../../../org/apache/bcel/verifier/exc/AssertionViolatedException.html">AssertionViolatedException</a>(<font color="#000099">"DID YOU REALLY WANT TO ASK FOR RET'S SUCCS?"</font>);
