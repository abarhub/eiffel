<a name="84" href="#84">84</a>    <b>public</b> <b>static</b> <b>final</b> <a href="../../../../org/apache/bcel/generic/ObjectType.html">ObjectType</a>    STRING       = <b>new</b> ObjectType(<font color="#000099">"java.lang.String"</font>);
<a name="85" href="#85">85</a>    <b>public</b> <b>static</b> <b>final</b> <a href="../../../../org/apache/bcel/generic/ObjectType.html">ObjectType</a>    STRINGBUFFER = <b>new</b> ObjectType(<font color="#000099">"java.lang.StringBuffer"</font>);
<a name="86" href="#86">86</a>    <b>public</b> <b>static</b> <b>final</b> <a href="../../../../org/apache/bcel/generic/ObjectType.html">ObjectType</a>    THROWABLE    = <b>new</b> ObjectType(<font color="#000099">"java.lang.Throwable"</font>);
<a name="87" href="#87">87</a>    <b>public</b> <b>static</b> <b>final</b> <a href="../../../../org/apache/bcel/generic/Type.html">Type</a>[]        NO_ARGS      = <b>new</b> Type[0];
