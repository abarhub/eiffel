<?xml version="1.0" encoding="ISO-8859-1" ?>
<?xml-stylesheet type="text/xsl" href="..\..\..\test_unitaire\programme.xsl"?>
<!DOCTYPE programme SYSTEM "..\..\..\test_unitaire\programme.dtd">
<!-- test si un parametre fait partie d'une expression -->
<programme>
	<classe>
		<type nom="TEST1"/>
		<attribut>
			<nom_attribut nom="make"/>
			<type nom="TEST1"/>
			<routine>
				<signature>
					<declare_var nom="k">
						<type nom="NONE"/>
					</declare_var>
				</signature>
				<declare_var nom="tmp">
					<type nom="NONE"/>
				</declare_var>
				<instruction>
					<instr_affect>
						<expression_scalaire type="var">
							<var nom="tmp"/>
						</expression_scalaire>
						<expression>
							<expression_scalaire type="var">
								<var nom="k"/>
							</expression_scalaire>
						</expression>
					</instr_affect>
				</instruction>
			</routine>
		</attribut>
		<attribut>
			<nom_attribut nom="Void"/>
			<type nom="ANY"/>
			<type nom="NONE"/>
		</attribut>
	</classe>
	<classe>
		<type nom="ANY"/>
	</classe>
	<classe>
		<type nom="NONE"/>
		<hierarchie>
			<type nom="TEST1"/>
			<type nom="ANY"/>
		</hierarchie>
	</classe>
</programme>
